{
  "ci": {
    "collect": {
      "numberOfRuns": 3,
      "staticDistDir": "./_site",
      "startServerCommand": "npm start"
      "url": "http://localhost:4000/"
    },
    "assert": {
      "preset": "lighthouse:recommended",
      "assertions": {
        "uses-webp-images": "off"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}